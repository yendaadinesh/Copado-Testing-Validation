    <validationRules>
        <fullName>PI_Hold</fullName>
        <active>false</active>
        <errorConditionFormula>AND(ISCHANGED(Override_PI_Referral_Status__c ),
(ISPICKVAL(PRIORVALUE(Override_PI_Referral_Status__c ), &quot;On Hold&quot;)),ISPICKVAL(Override_Study_Site_Recruitment_Status__c, &quot;On Hold&quot;) )</errorConditionFormula>
        <errorMessage>This Override PI Recruitment Status value cannot be changed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Site_On_Hold</fullName>
        <active>false</active>
        <errorConditionFormula>AND(ISCHANGED(Override_Study_Site_Recruitment_Status__c),

ISPICKVAL(PRIORVALUE(Override_Study_Site_Recruitment_Status__c), &quot;On Hold&quot;), 

ISPICKVAL(Clinical_Trial_Profile__r.Override_Recruitment_Status__c, &quot;On Hold&quot;))</errorConditionFormula>
        <errorMessage>This Override Study Site Recruitment Status value cannot be changed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Start_End_enrolment_date</fullName>
        <active>false</active>
        <errorConditionFormula>Enrollment_Start_Date__c  &gt;  Enrollment_End_Date__c</errorConditionFormula>
        <errorMessage>Enrollment End Date can&apos;t happen before Enrolment Start Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Study_Site_Status_Freeze</fullName>
        <active>false</active>
        <errorConditionFormula>AND(
ISCHANGED(Override_Study_Site_Recruitment_Status__c),
ISPICKVAL(PRIORVALUE(Override_Study_Site_Recruitment_Status__c),&quot;Enrollment Closed&quot;)
, Override_Status_Updated_By__c = &apos;IQVIA SPNL&apos;)</errorConditionFormula>
        <errorMessage>This Override Study Site Recruitment Status value cannot be changed</errorMessage>
    </validationRules>
