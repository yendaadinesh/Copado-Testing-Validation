        <fullName>Status_Category__c</fullName>
        <externalId>false</externalId>
        <formula>IF(OR(
ISPICKVAL(Participant_Status__c ,&apos;Identified&apos;),
ISPICKVAL(Participant_Status__c ,&apos;Contacted&apos;),
ISPICKVAL(Participant_Status__c ,&apos;Consulted&apos;),
ISPICKVAL(Participant_Status__c ,&apos;Interested&apos;),
ISPICKVAL(Participant_Status__c ,&apos;Pre-Eligibility&apos;),
ISPICKVAL(Participant_Status__c ,&apos;Opt-In Refer&apos;)), 
&apos;In Referral Process&apos;, 
IF(OR(
ISPICKVAL(Participant_Status__c ,&apos;Failed Referral&apos;),
ISPICKVAL(Participant_Status__c ,&apos;Failed to Enroll&apos;)),
&apos;Failed to Refer&apos;,
IF(OR(
ISPICKVAL(Participant_Status__c ,&apos;New Referral&apos;),
ISPICKVAL(Participant_Status__c ,&apos;Eligible&apos;),
ISPICKVAL(Participant_Status__c ,&apos;Visit Completed&apos;),
ISPICKVAL(Participant_Status__c ,&apos;Informed Consent&apos;),
ISPICKVAL(Participant_Status__c ,&apos;Screening&apos;)),
&apos;In Enrollment Process&apos;,
IF(OR(
ISPICKVAL(Participant_Status__c ,&apos;Enrolled&apos;),
ISPICKVAL(Participant_Status__c ,&apos;Randomized&apos;)),
&apos;Enrolled&apos;, &apos;Other&apos;))))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
