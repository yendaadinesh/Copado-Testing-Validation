        <searchResultsAdditionalFields>Condition_s_Therapeutic_Area__c</searchResultsAdditionalFields>
    </searchLayouts>
    <sharingModel>ReadWrite</sharingModel>
    <validationRules>
        <fullName>Prevent_Field_Updates_Integration</fullName>
        <active>true</active>
        <errorConditionFormula>!ISNEW()&amp;&amp;   (($User.FirstName + &apos; &apos; +  $User.LastName)  == &apos;IQVIA Integrations&apos;) &amp;&amp; (ISCHANGED(Brief_Summary__c)||ISCHANGED(MDM_Cluster_ID__c)||ISCHANGED(Randomization__c)||ISCHANGED(Study_Sponsor__c)||ISCHANGED(Condition_s_Therapeutic_Area__c)||ISCHANGED(Detailed_Description__c)||ISCHANGED(Eligibility_Criteria__c)||ISCHANGED(Estimated_Enrollment__c)||ISCHANGED(Responsible_Party_Organization__c)||ISCHANGED(NCT_Number__c)||ISCHANGED(Protocol_ID__c)||ISCHANGED(Responsible_Party__c)||ISCHANGED(Study_Arms__c)||ISCHANGED(Study_Design__c)||ISCHANGED(Study_Phase__c)||ISCHANGED(Study_Title__c)||ISCHANGED(Study_Type__c)||ISCHANGED(Start_Date__c)||ISCHANGED(Official_Title__c)||ISCHANGED(Study_Source__c)||ISCHANGED(US_FDA_Regulated_Product__c)||ISCHANGED(Has_Data_Monitoring_Committee__c)||ISCHANGED(Verification_Date__c)||ISCHANGED(Estimated_Completion_Date__c)||ISCHANGED(Enrollment_Target__c)||ISCHANGED(Estimated_Primary_Completion_Date__c)||ISCHANGED(Enrollment_Actual__c)||ISCHANGED(Study_Code_Name__c))</errorConditionFormula>
        <errorMessage>Only Status Field can be changed by Integration</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Recruitment_Status</fullName>
        <active>true</active>
        <description>SPNL can only change the value from &apos;Actively Enrolled&apos; to &apos;On Hold</description>
        <errorConditionFormula>AND(ISCHANGED(Override_Recruitment_Status__c),
ISPICKVAL(PRIORVALUE(Override_Recruitment_Status__c),&quot;Actively Enrolling&quot;), 
ISPICKVAL(Override_Recruitment_Status__c, &quot;On Hold&quot;)= FALSE,

$Profile.Name= &quot;IQVIA SPNL&quot;)</errorConditionFormula>
        <errorMessage>SPNL can only change the value from &apos;Actively Enrolled&apos; to &apos;On Hold</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Start_End_enrollment_trial_date</fullName>
        <active>false</active>
        <errorConditionFormula>Enrollment_Start_Date__c  &gt;  Enrolled_End_Date__c</errorConditionFormula>
        <errorMessage>Enrollment End Date can&apos;t happen before Enrolment Start Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Type_of_NCT_number</fullName>
        <active>false</active>
        <errorConditionFormula>OR(LEN(NCT_Number__c) &lt;&gt; 11,
NOT REGEX(NCT_Number__c, &quot;NCT\\d{8}&quot;))</errorConditionFormula>
        <errorDisplayField>NCT_Number__c</errorDisplayField>
        <errorMessage>NCT number should have a format &quot;NCT00000000&quot;</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>
